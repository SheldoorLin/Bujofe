<?xml version="1.0" encoding="utf-8"?>

<!--Calendar.Timeline Settings-->
<resources>
    <declare-styleable name="TimeLineLayout">
        <attr name="numberOfRows" format="integer"/>
        <attr name="eachRowHeight" format="dimension"/>
        <attr name="minimumPercentage" format="float"/>
        <attr name="dividerColor" format="color"/>
        <attr name="dividerTextColor" format="color"/>
        <attr name="dividerTitles"/>
        <attr name="dividerStartOffset" format="dimension"/>
    </declare-styleable>


    <declare-styleable name="SeatTableView">
        <attr name="seat_checked" format="reference" />
        <attr name="seat_sold" format="reference" />
        <attr name="seat_available" format="reference" />
        <attr name="overview_checked" format="color" />
        <attr name="overview_sold" format="color" />
        <attr name="txt_color" format="color"/>
    </declare-styleable>

</resources>